<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  

  
  <servlet>
    <description>婚纱店的首页</description>
    <display-name>婚纱店的首页</display-name>
    <servlet-name>index</servlet-name>
    <servlet-class>ods.index</servlet-class>
  </servlet>
  <servlet>
    <description>读取婚纱信息</description>
    <display-name>读取婚纱信息</display-name>
    <servlet-name>dressshow</servlet-name>
    <servlet-class>ods.dressshow</servlet-class>
  </servlet>
  <servlet>
    <description>读取页面的用户注册信息</description>
    <display-name>读取页面的用户注册信息</display-name>
    <servlet-name>register</servlet-name>
    <servlet-class>ods.register</servlet-class>
  </servlet>
  <servlet>
    <description>读取页面的用户登录信息</description>
    <display-name>读取页面的用户登录信息</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>ods.login</servlet-class>
  </servlet>
  <servlet>
    <description>添加商品到购物车</description>
    <display-name>添加商品到购物车</display-name>
    <servlet-name>cart</servlet-name>
    <servlet-class>ods.cart</servlet-class>
  </servlet>
  <servlet>
    <description>读取页面的管理员登录信息</description>
    <display-name>读取页面的管理员登录信息</display-name>
    <servlet-name>adminlogin</servlet-name>
    <servlet-class>ods.adminlogin</servlet-class>
  </servlet>
  <servlet>
    <description>文件上传</description>
    <display-name>文件上传</display-name>
    <servlet-name>fileuploadservlet</servlet-name>
    <servlet-class>ods.fileuploadservlet</servlet-class>
  </servlet>
  <servlet>
    <description>管理员登录后对商品信息进行维护</description>
    <display-name>管理员登录后对商品信息进行维护</display-name>
    <servlet-name>admindress</servlet-name>
    <servlet-class>ods.admindress</servlet-class>
  </servlet>
  <servlet>
    <description>管理员婚纱信息编辑页面</description>
    <display-name>管理员婚纱信息编辑页面</display-name>
    <servlet-name>editdress</servlet-name>
    <servlet-class>ods.editdress</servlet-class>
  </servlet>
  <servlet>
    <description>婚纱信息删除页面</description>
    <display-name>婚纱信息删除页面</display-name>
    <servlet-name>deletedress</servlet-name>
    <servlet-class>ods.deletedress</servlet-class>
  </servlet>
  <servlet>
    <description>购物车删除页面</description>
    <display-name>购物车删除页面</display-name>
    <servlet-name>deletecart</servlet-name>
    <servlet-class>ods.deletecart</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dressshow</servlet-name>
    <url-pattern>/dressshow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>adminlogin</servlet-name>
    <url-pattern>/adminlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fileuploadservlet</servlet-name>
    <url-pattern>/fileuploadservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>admindress</servlet-name>
    <url-pattern>/admindress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>editdress</servlet-name>
    <url-pattern>/editdress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deletedress</servlet-name>
    <url-pattern>/deletedress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deletecart</servlet-name>
    <url-pattern>/deletecart</url-pattern>
  </servlet-mapping>		
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
</web-app>
