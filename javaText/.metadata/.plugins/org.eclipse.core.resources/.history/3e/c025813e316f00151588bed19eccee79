package com.remix;
//计算任一个数的因子  病保存带数组中   遍历数组

import java.util.Scanner;

public class Day06_4_1_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		yinzi.yin(9);
		
	}

}
class yinzi{
	public static void yin(int a){
		
		int sum=0;
		for (int i = 1; i <= a; i++) {
			if (a%i == 0 && a != i) {
				
				sum+= i;
			}
		}
		System.out.println(sum);
	}
	
}
